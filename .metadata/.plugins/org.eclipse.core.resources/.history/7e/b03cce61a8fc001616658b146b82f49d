

public class Node1{
	Node1 next = null;
	int data;
	
	public Node1(int d){
		data = d;
	}
	
	public void appendToTail(int d){
		Node1 end = new Node1(d);
		Node1 n=this;
		
		while(n.next != null){
			n= n.next;
		}
		n.next = end;
		System.out.println(n.data);
	}
	
	public void main(String[]args){
		
		
		appendToTail(5);
		appendToTail(4);
		appendToTail(3);
		appendToTail(2);
		appendToTail(1);
		
		
	}
}