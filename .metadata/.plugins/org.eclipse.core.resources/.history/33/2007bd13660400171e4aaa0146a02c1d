import org.w3c.dom.Node;

public class traversals{


	public void preOrder(Node root){
		System.out.print(root.data+ " ");
		if(root.left != null){
	        preOrder(root.left);
	    }
	    if(root.right!=null){
	        preOrder(root.right);
	    }
	}
	
	public void postOrder(Node root) {
	    
	    if(root.left != null){
	        postOrder(root.left);
	    }
	    if(root.right!=null){
	        postOrder(root.right);
	    }
	    
	    System.out.print(root.data + " ");
	
	}

}